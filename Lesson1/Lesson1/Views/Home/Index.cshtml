@model IEnumerable<Lesson1.Models.Students>

@{
    ViewData["Title"] = "All Students";
}

<!doctype html>
<html>
<head>
	<meta name="viewport" content="width=device-width" charset="utf-8" />
	<title>@ViewData["Title"]</title>
</head>
<body>
<table>
	<tr>
		<td>РШФ Id</td>
		<td>Фамилия</td>
		<td>Имя</td>
		<td>Возраст</td>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@Html.ActionLink(item.Id, "Details", new { id = item.Id })</td>
			<td>@item.LastName</td>
			<td>@item.FirstName</td>
			<td>@item.Age</td>
		</tr>
	}
</table>
</body>
</html>

